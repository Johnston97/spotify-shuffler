{
  "&::-webkit-scrollbar": {
    "width": "10px"
  },
  "&::-webkit-scrollbar-track": {
    "width": "6px"
  },
  "&::-webkit-scrollbar-thumb": {
    "background": "transparent",
    "borderRadius": "24px"
  },
  ":hover": {
    "&::-webkit-scrollbar-thumb": {
      "background": "Grey"
    }
  }
}
